capsule {
  id (example.meow)
  version (0.2.0)
  format (3)
  targets {
    target (bixby-mobile-en-US)
  }
  runtime-version (2)
  capsule-imports {
    import (bixby.audioPlayer) {
      version (1.2.1)
      as (audioPlayer)
    }
  }
  store-sections {
    section (MusicAndAudio)
  }
  store-countries {
    all
  }
}